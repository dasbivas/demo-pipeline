pipeline {
    agent any
    stages {
        stage('Build Stage') {
            steps {
                echo 'This is build Stage'
            }
        }
        stage('Push Stage') {
            steps {
                echo 'This is push stage to registry'
            }
        }
        stage('Test Stage') {
            steps {
                echo 'This is test stage'
            }
        }
        stage('Integration Stage') {
            steps {
                echo 'This is Integration Stage'
            }
        }
        stage('Deploy Stage') {
            steps {
                echo 'This is Deploy stage'
            }
        }
    }
}
