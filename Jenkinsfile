pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'This is build Stage'
            }
        }
        stage('Push') {
            steps {
                echo 'This is push stage to registry'
            }
        }
        stage('Test') {
            steps {
                echo 'This is test stage'
            }
        }
        stage('Integration') {
            steps {
                echo 'This is Integration Stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'This is Deploy stage'
            }
        }
    }
}
